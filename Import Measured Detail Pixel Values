Sub Import_BKG_Data()
'
' Import_BKG_Data Macro
' Import BKG Data
'

'
    ActiveWindow.SmallScroll Down:=-21
    Workbooks.Open Filename:="E:\MTW\MSc\MSc Project\TO20\ROI Data\ROI BKG.csv"
    Range("C2:D12").Select
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("D6").Select
    ActiveSheet.Paste
    Sheets("Import Data").Select
    Windows("ROI BKG.csv").Activate
    Range("C2:D13").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("D6").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    Range("C14:D25").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("J6").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=6
    Range("C26:D37").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("P6").Select
    ActiveSheet.Paste
    ActiveWindow.SmallScroll Down:=9
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=21
    Range("C38:D49").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("D21").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=6
    Range("C50:D61").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("J21").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=15
    Range("C62:D73").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("P21").Select
    ActiveSheet.Paste
    ActiveWindow.SmallScroll Down:=9
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=9
    Range("C74:D85").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("D36").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=12
    Range("C86:D97").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("J36").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=15
    Range("C98:D109").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("P36").Select
    ActiveSheet.Paste
    ActiveWindow.SmallScroll Down:=15
    Windows("ROI BKG.csv").Activate
    Range("C110:D121").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("D51").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=15
    Range("C122:D133").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("J51").Select
    ActiveSheet.Paste
    Windows("ROI BKG.csv").Activate
    ActiveWindow.SmallScroll Down:=15
    Range("C134:D145").Select
    Application.CutCopyMode = False
    Selection.Copy
    Windows("Automated Scoring.xlsm").Activate
    Range("P51").Select
    ActiveSheet.Paste
End Sub
